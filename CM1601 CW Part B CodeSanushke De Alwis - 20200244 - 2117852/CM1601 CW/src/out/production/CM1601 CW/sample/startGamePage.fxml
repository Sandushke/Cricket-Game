            <Font name="Times New Roman Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="quitButton" blendMode="DARKEN" layoutX="518.0" layoutY="433.0" mnemonicParsing="false" onAction="#view_close_fn" prefHeight="34.0" prefWidth="80.0" text="Quit">
